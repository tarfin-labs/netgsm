{
    "name": "tarfin-labs/netgsm",
    "description": "netgsm channel for laravel",
    "keywords": [
        "tarfin-labs",
        "netgsm"
    ],
    "homepage": "https://github.com/tarfin-labs/netgsm",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "hakan özdemir",
            "email": "hakan.ozdemir@tarfin.com",
            "role": "Developer"
        },
        {
            "name": "Faruk Can",
            "email": "faruk.can@tarfin.com",
            "role": "Developer"
        },
        {
            "name": "Yunus Emre Deligöz",
            "email": "emre.deligoz@tarfin.com",
            "role": "Developer"
        },
        {
            "name": "Turan Karatuğ",
            "email": "turan.karatug@tarfin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/support": "^8.0",
        "illuminate/notifications": "^8.0",
        "illuminate/translation": "^8.0",
        "nesbot/carbon": "^2.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TarfinLabs\\Netgsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TarfinLabs\\Netgsm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TarfinLabs\\Netgsm\\NetgsmServiceProvider"
            ],
            "aliases": {
                "Netgsm": "TarfinLabs\\Netgsm\\NetgsmFacade"
            }
        }
    }
}
